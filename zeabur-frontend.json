{
  "name": "picture-book-creator-frontend",
  "type": "nodejs",
  "buildCommand": "npm install && npm run build",
  "startCommand": "npm run preview",
  "environment": {
    "NODE_ENV": "production"
  },
  "domains": [],
  "port": 4173,
  "healthCheck": {
    "path": "/",
    "interval": 30,
    "timeout": 10,
    "retries": 3
  },
  "resources": {
    "cpu": "0.5",
    "memory": "512Mi"
  },
  "autoDeploy": {
    "enabled": true,
    "branch": "main"
  }
} 